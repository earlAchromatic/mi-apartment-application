{
  "name": "ionia-apartments-app",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"vite\" \"tsc --noEmit --watch\"",
    "dev:functions": "concurrently \"vite\" \"tsc --noEmit --watch\" \"npm run tx\" \"netlify functions:serve\"",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tx": "swc functions-src/send-pdf.js -o netlify/functions/send-pdf.js -w -s inline"
  },
  "dependencies": {
    "busboy": "^1.5.0",
    "chart.js": "^3.7.0",
    "formidable": "^3.2.1",
    "html2pdf.js": "^0.9.2",
    "jspdf": "^2.5.1",
    "nodemailer": "^6.7.3",
    "primeflex": "^3.1.2",
    "primeicons": "^5.0.0",
    "primevue": "^3.11.0",
    "quill": "^1.3.7",
    "vue": "^3.2.25",
    "vue-gtag": "^2.0.1",
    "vue-router": "^4.0.12",
    "vue-signature-pad": "^3.0.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.56",
    "@swc/core": "^1.2.160",
    "@vitejs/plugin-vue": "^2.0.0",
    "chokidar": "^3.5.3",
    "concurrently": "^7.0.0",
    "sass": "^1.49.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  }
}
